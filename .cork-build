{
  "registry" : "us-west1-docker.pkg.dev/digital-ucdavis-edu/wordpress",

  "repositories" : {
    "ucdlib-theme-wp" : "https://github.com/UCDavisLibrary/ucdlib-theme-wp"
  },

  "images" : {
    "ucdlib-intranet-ucdlib-theme-wp" : {
      "contextPath" : "dockerfiles",
      "dockerfile" : "dockerfiles/Dockerfile.ucdlib-theme-wp",
      "options" : {
        "build-arg" : [
          "NODE_VERSION=20",
          "UCDLIB_THEME_WP_SRC_IMAGE=${ucdlib-theme-wp.ucdlib-theme-wp}"
        ]
      }
    },
    "ucdlib-intranet" : {
      "contextPath" : ".",
      "dockerfile" : "dockerfiles/Dockerfile",
      "options" : {
        "build-arg" : [
          "NODE_VERSION=20",
          "UCDLIB_THEME_WP_NODE_IMAGE=${ucdlib-intranet.ucdlib-intranet-ucdlib-theme-wp}"
        ],
        "secret" : [
          "id=google_key,env=ITIS_BACKUP_READER_KEY"
        ]
      }
    },
    "ucdlib-intranet-utils" : {
      "contextPath" : "./utils",
      "options" : {
        "build-arg" : [
          "NODE_VERSION=20",
          "BASE_IMAGE=${ucdlib-intranet.ucdlib-intranet}"
        ]
      }
    }
  }
}
